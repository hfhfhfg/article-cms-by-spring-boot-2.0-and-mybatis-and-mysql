<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sbs.starter.dao.MemberDao">
	<select id="getLoginIdDupCount" parameterType="String" resultType="int">
		SELECT Count(*) FROM member WHERE loginId = #{loginId}
	</select>
	
	<insert id="join" parameterType="map" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO member(regDate,loginId,loginPw) values(now(),#{loginId},#{loginPw})
	</insert>
	
	
</mapper>